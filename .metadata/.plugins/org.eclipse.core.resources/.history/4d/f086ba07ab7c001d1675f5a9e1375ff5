
public class Variable {

	public static void main(String[] args) {
		
		int x = 123565;//initialization
		
		double y=33.9999;
		
		float e =2.14f;
		
		boolean z= false;
		
		
		System.out.println("my int  is "+x);
		System.out.println("my double  is "+y);
		System.out.println("my float is "+e);
		System.out.println("the boolean is" + z);
		

	}

}
